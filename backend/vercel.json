{
  "github": {
    "silent": true
  },
  "version": 2,
  "builds": [
    {
      "src": "/src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    }
  ],
  "regions": [
    "sin1"
  ]
}